<button type="button" class="sort btn btn-primary btn-lg">Sort</button>
<br/><br/>

<table class="table table-bordered">
	<thead>
		<th></th>
		<th>Points</th>
		<th>Looks</th>
		<th>Name</th>
		<th>Ship</th>
	</thead>
	<tbody>
	<% can.each(commanders, function(commander) { %>
	<tr <%= (el) -> $(el).data('commander', commander) %>>
		<td class="buttons">
			<a href="javascript://" class="up"></a>
			<a href="javascript://" class="down"></a>
		</td>
		<td class="vote">
			<span class="<%= commander.attr('votes') > 0 ? 'green' : 'red' %> toggle"><%= commander.attr('votes') %></span>
			<div class="toggle" style="display: none;">
				<span class="green"><%= commander.attr('upvotes') %></span><br />
				<span class="red"><%= commander.attr('downvotes') %></span>
			</div>
		</td>
		<td class="photo">
			<img src="<%= commander.photo %>"/>
		</td>
		<td class="large"><%= commander.attr('name') %></td>
		<td class="large"><%= commander.attr('ship') %></td>
	</tr>
	<% }) %>
	</tbody>
</table>